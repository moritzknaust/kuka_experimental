<ETHERNETKRL>
   <CONFIGURATION>
      <EXTERNAL>
         <TYPE>Client</TYPE>                  <!-- Users connect as clients -->
      </EXTERNAL>
      <INTERNAL>
         <ENVIRONMENT>Program</ENVIRONMENT>   <!-- Server run via robot interpreter -->
         <BUFFERING Limit="512" />            <!-- Allow buffering of up to 512 messages (system max) -->
         <ALIVE Set_Flag="1" />               <!-- Use $flag[1] to indicate alive/good connection status -->
         <IP>172.16.99.99</IP>                <!-- IP address for EKI interface on robot controller (KRC) -->
         <PORT>54600</PORT>                   <!-- Port of EKI interface on robot controller (in [54600, 54615]) -->
         <PROTOCOL>UDP</PROTOCOL>             <!-- Use UDP protocol -->
      </INTERNAL>
   </CONFIGURATION>

   <!-- Configured XML structure for data reception (external client to robot)
        <RobotCommand>
           <Pos A1="..." A2="..." A3="..." A4="..." A5="..." A6="...">
           </Pos>
        </RobotCommand>
   -->
   <RECEIVE>
      <XML>
         <!-- Joint position command -->
         <ELEMENT Tag="RobotCommand/Pos/@A1" TYPE="REAL" INDX="1" UNIT="0" HOLDON="1"/>
         <ELEMENT Tag="RobotCommand/Pos/@A2" TYPE="REAL" INDX="1" UNIT="0" HOLDON="1"/>
         <ELEMENT Tag="RobotCommand/Pos/@A3" TYPE="REAL" INDX="1" UNIT="0" HOLDON="1"/>
         <ELEMENT Tag="RobotCommand/Pos/@A4" TYPE="REAL" INDX="1" UNIT="0" HOLDON="1"/>
         <ELEMENT Tag="RobotCommand/Pos/@A5" TYPE="REAL" INDX="1" UNIT="0" HOLDON="1"/>
         <ELEMENT Tag="RobotCommand/Pos/@A6" TYPE="REAL" INDX="1" UNIT="0" HOLDON="1"/>
      </XML>
   </RECEIVE>

   <!-- Configured XML structure for data transmission (robot to external client)
        <RobotState>
           <Pos A1="..." A2="..." A3="..." A4="..." A5="..." A6="...">
           </Pos>
           <Vel A1="..." A2="..." A3="..." A4="..." A5="..." A6="...">
           </Vel>
           <Eff A1="..." A2="..." A3="..." A4="..." A5="..." A6="...">
           </Eff>
        </RobotState>
   -->
   <SEND>
      <XML>
         <!-- Joint state positions -->
         <ELEMENT Tag="RobotState/Pos/@A1" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Pos/@A2" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Pos/@A3" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Pos/@A4" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Pos/@A5" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Pos/@A6" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <!-- Joint state velocities -->
         <ELEMENT Tag="RobotState/Vel/@A1" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Vel/@A2" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Vel/@A3" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Vel/@A4" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Vel/@A5" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Vel/@A6" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <!-- Joint state efforts (torques) -->
         <ELEMENT Tag="RobotState/Eff/@A1" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Eff/@A2" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Eff/@A3" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Eff/@A4" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Eff/@A5" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <ELEMENT Tag="RobotState/Eff/@A6" TYPE="REAL" INDX="INTERNAL" UNIT="0"/>
         <!-- Interface state -->
         <ELEMENT Tag="RobotState/RobotCommand/@Size"  TYPE="REAL" INDX="INTERNAL" UNIT="0"/>  <!-- Number of elements currently in command buffer -->
      </XML>
   </SEND>
</ETHERNETKRL>
